@{
    Layout = null;
}

@model WebLacome.Models.User
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <title>DangNhap</title>
    <style>
        .background {
            background-size: cover;
            background-image: url(data:image/jpeg;base64,);
        }

        .thietke {
            border: 2px solid blue;
            width: 70%;
            margin: 30px auto;
        }

        .tieude {
            background: rgba(0, 0, 0, 0.8);
            font-family: 'Roboto Mono', monospace;
            color: aliceblue;
            text-align: center;
            padding: 40px 0px;
            font-size: 30px;
            text-transform: uppercase;
            font-family: Comic Sans MS;
        }

        .khung {
            font-size: 20px;
            padding-top: 40px;
            background: LightGoldenrodYellow;
        }

        .DangNhap {
            display: flex;
        }

        ul {
            list-style: none;
        }

        li {
            padding: 10px;
        }

        .input_dien {
            width: 90%;
        }

        .thongtin {
            width: 30%;
        }

        .dien {
            width: 70%;
        }

        .cuoi {
            text-align: center;
            padding: 5px 0px;
        }

        button {
            border-radius: 10px;
            padding: 5px;
        }

            button a {
                color: black;
                text-decoration: none;
                font-size: 20px;
            }
    </style>
</head>
<body class="background">
    <form action="/Login/DangNhap" method="post">
        <div class="thietke">
            <div class="tieude">
                THÔNG TIN ĐĂNG NHẬP ADMIN
            </div>
            <div class="khung">
                <div class="DangNhap">
                    <ul class="thongtin">
                        <i class="glyphicon glyphicon-cloud"> Name</i></br>
                        <i class="glyphicon glyphicon-envelope" style="padding-top: 30px;"> Email</i></br>
                        <i class="glyphicon glyphicon-user" style="padding-top: 30px;"> User Name</i></br>
                        <i class="glyphicon glyphicon-user" style="padding-top: 30px;"> Password</i>
                    </ul>
                    <ul class="dien">
                        <li><input type="text" class="input_dien" id="name" name="name" /></li>
                        <li><input type="email" class="input_dien" id="email" name="email" /></li>
                        <li><input type="text" class="input_dien" id="username" name="username" /></li>
                        <li><input type="password" class="input_dien" id="password" name="password" /></li>
                    </ul>

                </div>
                <div class="cuoi">
                    <input type="checkbox" />Lưu thông tin
                </div>
                <div class="cuoi">
                    <button>Đăng Nhập</button>
                </div>

            </div>
        </div>
    </form>

</body>
</html>



